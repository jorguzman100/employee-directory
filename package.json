{
  "name": "employee-atlas",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite --config client/vite.config.mjs",
    "dev:server": "NODE_ENV=development nodemon --watch server --ext js,cjs,mjs,json server/index.js",
    "build": "vite build --config client/vite.config.mjs",
    "preview": "vite preview --config client/vite.config.mjs",
    "start": "NODE_ENV=production node server/index.js",
    "test": "vitest run --config client/vite.config.mjs",
    "test:watch": "vitest --config client/vite.config.mjs"
  },
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.1",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "vite": "^6.4.1",
    "vitest": "^4.0.18"
  }
}
